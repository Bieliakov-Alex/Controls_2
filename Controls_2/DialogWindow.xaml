<Window x:Class="Controls_2.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Controls_2"
        mc:Ignorable="d"
        Title="DialogWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <TextBox Text="EmptyTextBox" Name="TextBoxString" />
        <Button Content="Ok" IsDefault="True" Click="Ok_Click" />
        <Button Content="Cancel" IsCancel="True" />
    </StackPanel>
</Window>
